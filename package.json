  },
  "homepage": "github.com/neilff/redux-query#readme",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babelify": "6.4.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.13.1",
    "redux": "^3.0.5",
    "rimraf": "^2.5.0",
    "tap-spec": "^4.1.1",
    "tap-xunit": "^1.3.1",
    "tape": "^4.3.0",
    "tape-run": "^2.1.0",
